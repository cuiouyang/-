  <select id="getIdBySeriaNo" resultType="com.bsi.ms.model.ProjectMember" parameterType="java.lang.String" >
  select id projectId,pm_account memberAccount,pm_name memberName from project where seria_no=#{seriaNo}
  </select>

  <select id="queryByTerm" resultMap="BaseResultMap" parameterType="java.util.Map" >
   select 
    <include refid="partake_item" />
    from project_member pm,project as p left join projectms.project_view as pv on pv.id = p.id
    left join work_item w on w.project_id = p.id  
    and w.status = 1 and w.creator_account =#{creName}
    where pm.project_id=p.id and p.pigeonhole=0  
    <if test="projectName != null and projectName != ''">
		and p.project_name like '%${projectName}%'
	</if> 
	<if test="pmName != null and pmName != ''">
		and p.pm_name like '%${pmName}%'
	</if>
	<if test="sta != null and sta != ''">
		and pm.status = #{sta}
	</if> 
	<if test="memberAccount != null and memberAccount != ''">
		and pm.member_account = #{memberAccount}
	</if> 
	<if test="opt == 1">  
     <![CDATA[ and  p.start_time between #{startTime} and  #{endTime}     ]]> 
    </if>
    <if test="opt == 2">  
     <![CDATA[ and  p.end_time between #{startTime} and  #{endTime}     ]]> 
    </if>
	group by p.id 
  </select> 
  
  <select id="newItemsValidateName" parameterType="java.lang.String" resultType ="java.lang.Integer">
