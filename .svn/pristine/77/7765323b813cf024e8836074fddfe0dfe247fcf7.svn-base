      </if> 
       <if test="status!=100 and status!='' and status!=null" >
       and w.status = #{status,jdbcType=INTEGER}
      </if>
       <if test="projectId != '' and projectId != null" >
       and w.project_id =#{projectId,jdbcType=INTEGER}
      </if>  
  </select>
  
