  <select id="selectByProject" resultMap="BaseResultMap" parameterType="map" >
    select  p.*,d.dict_name projectTypeName,pv.progressBar progressBar
    from project p join dict d on p.project_type=d.dict_value and d.parent_id=1,
         project as p1 LEFT JOIN projectms.project_view as pv  on pv.id = p1.id
   where d.status=1 and p.id=p1.id
    <if test="type == 1">  
     <![CDATA[ and  p.project_name like CONCAT('%',#{value},'%')    ]]> 
     </if>  
     <if test="type == 2">  
     <![CDATA[ and  p.pm_name like CONCAT('%',#{value},'%')    ]]> 
      </if>  
      <if test="type == 3">  
     <![CDATA[ and  p.resp_dept_name like CONCAT('%',#{value},'%')    ]]> 
      </if>
      <if test="type == 4">  
     <![CDATA[ and  p.creator_name like CONCAT('%',#{value},'%')    ]]> 
      </if>
      <if test="type == 5">  
     <![CDATA[ and  d.dict_name like CONCAT('%',#{value},'%')     ]]> 
      </if>
       <if test="type == 6">  
     <![CDATA[ and  p.seria_no like CONCAT('%',#{value},'%')     ]]> 
      </if>
       <if test="date == 1">  
     <![CDATA[ and  p.start_time between #{startDate} and #{endDate}   ]]> 
	  </if>  
	  <if test="date == 2">  
	   <![CDATA[ and  p.end_time between #{startDate} and #{endDate}   ]]> 
	  </if> 
     
  </select>
