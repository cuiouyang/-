  <select id="selectByMyWork" resultMap="ResultMapWithBLOBs" parameterType="com.bsi.ms.model.WorkItem" >
     <include refid="IAssume"/>
     <if test="itemTopic != '' and itemTopic != null" >
      and  w.item_topic like '%${itemTopic}%' 
      </if> 
      <if test="projectName != '' and projectName != null" >
